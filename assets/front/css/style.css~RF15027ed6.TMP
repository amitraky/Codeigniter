---///---///---///---///---///---///---///---///---///---*/
.contact-section {
    overflow: hidden;
    padding: 100px 0 70px;
}
.contact-title h2 {
    font-size: 36px;
    margin-bottom: 20px;
    text-transform: uppercase;
