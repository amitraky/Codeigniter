    letter-spacing: 8px;
    font-style: italic;
    color: #6AB4FF;
    text-transform: uppercase;
}
.error-col h3 {
    font-size: 40px;
    text-transform: uppercase;
    margin-bottom: 20px;
}
.error-col img {
    margin-bottom: 30px;
}
.error-col a {
    font-size: 30px;
    color: #6AB4FF;
    padding: 8px 20px;
}
.error-col a:hover {
    color: #ffffff;
    background: #6AB4FF;
}



